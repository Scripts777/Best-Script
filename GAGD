local PET_VALUES2 = {
    ["French Fry Ferret"] = 300,
    ["Raccoon"] = 400,
    ["T-Rex"] = 100,
    ["Fennec Fox"] = 300,
    ["Dragonfly"] = 150,
    ["Butterfly"] = 300,
    ["Disco Bee"] = 500,
    ["Mimic Octopus"] = 100,
    ["Queen Bee"] = 100,
